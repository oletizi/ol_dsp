{
  "name": "@oletizi/live-max-cc-router",
  "version": "1.20.0",
  "description": "TypeScript-based CC router for Max for Live with controller-specific canonical mappings",
  "type": "module",
  "main": "./dist/cc-router.js",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/cc-router.js"
    }
  },
  "files": [
    "dist",
    "src",
    "scripts",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run convert-maps && node node_modules/rollup/dist/bin/rollup -c",
    "build:tsc": "tsc -b",
    "watch": "node node_modules/rollup/dist/bin/rollup -c -w",
    "dev": "node scripts/watch-deploy.cjs",
    "clean": "rm -rf dist *.tsbuildinfo",
    "convert-maps": "node scripts/convert-canonical-maps.cjs",
    "deploy": "pnpm run build && node scripts/deploy.cjs",
    "deploy:initial": "node scripts/deploy.cjs",
    "setup": "node scripts/setup.cjs",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "verify-mapping": "node scripts/ai-verify-mapping.cjs",
    "verify-mapping:channev": "node scripts/ai-verify-mapping.cjs ../canonical-midi-maps/maps/novation-launch-control-xl-3/channel-strips/analog-obsession-channev.yaml ../canonical-midi-maps/plugin-descriptors/analogobsession-channev.json",
    "fix-mapping:channev": "node scripts/ai-verify-mapping.cjs ../canonical-midi-maps/maps/novation-launch-control-xl-3/channel-strips/analog-obsession-channev.yaml ../canonical-midi-maps/plugin-descriptors/analogobsession-channev.json --fix",
    "verify-all": "node scripts/verify-all-mappings.cjs",
    "fix-all": "node scripts/verify-all-mappings.cjs --fix",
    "import-from-device": "node scripts/import-from-device.cjs"
  },
  "keywords": [
    "max-for-live",
    "midi",
    "ableton-live",
    "launch-control-xl3",
    "typescript",
    "canonical-midi-maps",
    "controller"
  ],
  "author": "",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@oletizi/canonical-midi-maps": "workspace:*",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.11.0",
    "chokidar": "^3.5.3",
    "rollup": "^2.79.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
