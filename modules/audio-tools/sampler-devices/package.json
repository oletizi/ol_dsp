{
  "name": "@oletizi/sampler-devices",
  "version": "3.0.0",
  "description": "Module for communicating with old midi devices",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./s3k": {
      "types": "./dist/s3k.d.ts",
      "import": "./dist/s3k.js",
      "require": "./dist/s3k.cjs"
    },
    "./s5k": {
      "types": "./dist/s5k.d.ts",
      "import": "./dist/s5k.js",
      "require": "./dist/s5k.cjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "gen": "tsx src/gen-s3000xl.ts src/devices",
    "test": "c8 mocha",
    "integration": "c8 mocha test/integration/**/*.test.ts",
    "build": "npm test && tsup",
    "clean": "rm -rf dist"
  },
  "author": "Orion Letizi",
  "license": "Apache-2.0",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.8",
    "@types/midi": "^2.0.3",
    "@types/node": "^22.13.1",
    "@types/sinon": "^17.0.3",
    "c8": "^10.1.3",
    "chai": "^5.1.2",
    "mocha": "^11.1.0",
    "mochawesome": "^7.1.3",
    "sinon": "^19.0.2",
    "tmpdir": "^1.0.1",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "yaml": "^2.7.0"
  },
  "dependencies": {
    "@oletizi/sampler-lib": "^1.0.11",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10"
  }
}
