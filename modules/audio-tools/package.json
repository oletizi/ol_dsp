{
  "name": "@oletizi/audio-tools",
  "version": "2.0.0",
  "private": false,
  "description": "Monorepo for hardware sampler utilities and format parsers",
  "type": "module",
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "pnpm -r build",
    "clean": "pnpm -r clean",
    "test": "pnpm -r test",
    "prepublishOnly": "tsx scripts/copy-license.ts",
    "version:bump": "tsx scripts/bump-version.ts",
    "publish:modules": "tsx scripts/publish-modules.ts",
    "release": "tsx scripts/release.ts",
    "dev": "next dev"
  },
  "keywords": [
    "audio",
    "sampler",
    "akai",
    "midi",
    "parser"
  ],
  "author": "Orion Letizi",
  "license": "Apache-2.0",
  "devDependencies": {
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
